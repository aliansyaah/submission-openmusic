{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "indent": "off",
        "comma-dangle": "off",
        // "comma-dangle": [2, "always-multiline"],
        "code": 200     // max length code in one line
        // "code": false
    }
}
